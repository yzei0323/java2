package day1prac.제네릭클래스;

public class BoxMain {

	public static void main(String[] args) {
		
		
		Box box = new Box();
		box.setItem("문자열담기");
		String item = (String)box.getItem();

		System.out.println(item);

		
		Box box2 = new Box();
		box2.setItem(new Acorn("KJU", "권지언"));
		Acorn acorn = (Acorn)box2.getItem();
	}

	
	
	
}
