package day1prac.제네릭매서드;

public class 제네릭연습하기Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		 int result  =제네릭매서드연습.선택하기int(5, 7);
		 System.out.println( result);
		 
		 
		 String result2 =제네릭매서드연습.선택하기String("산", "바다");
		 System.out.println(  result2);
		 
		 
		 
		 
		boolean result3= 제네릭매서드연습.선택하기boolean(false, true);
		System.out.println( result3);
		 
		 		
		//쉬고 나서 제네릭 매서드로 만들어보세요 !
		
		String  result4 =제네릭매서드연습.<String>선택하기("석양", "일출");
		int  result5 =제네릭매서드연습.<Integer>선택하기(3,7);
		Acorn acron  =제네릭매서드연습.<Acorn>선택하기(new Acorn("kmh","김민환" ,100),
				                             new Acorn("kym","김유민" ,200));
		

	}

}
